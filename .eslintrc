{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            0
        ],
        "no-unused-vars": [
            0
        ]
    },
    "env": {
        "node": true,
        "browser": true,
        "amd": true
    },
    "extends": "eslint:recommended"
}